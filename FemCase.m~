classdef FemCase < handle
    properties
        mesh
        physics
        loads
        bc
        dis
        reactions
    end
    methods
        function obj = FemCase(mesh_in,physics)
            require(isa(mesh_in,'Mesh'), ...
                'ArgumentError: mesh_in not type Mesh');
            require(isa(physics,'Physics'), ...
                'ArgumentError: mesh_in not type Physics');
            obj.mesh = mesh_in;
            obj.physics = physics;
            
            obj.loads =          CompoundFunction(true,mesh.n_nodes,physics.dofs_per_node, ...
                mesh.n_ele,physics.dofs_per_ele)
            femcase.set('loads', Loads(total_number_of_nodes_in,dofs_per_node_in, ...
                            total_number_of_elements_in, dofs_per_element_in))
            femcase.set('displacements', Displacements(total_number_of_nodes_in, ...
                            dofs_per_node_in, total_number_of_elements_in, ...
                            dofs_per_element_in))
            femcase.set('reactions', Reactions(total_number_of_nodes_in, ...
                            dofs_per_node_in, total_number_of_elements_in, ...
                            dofs_per_element_in))
        end
    end
end