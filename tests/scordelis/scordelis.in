FILEECHO OPTION=FILE FILE=log.log
FILELOG  OPTION=FILE FILE=log.log

PARAMETER R '25'
PARAMETER L '25'
PARAMETER t '0.025'
PARAMETER a '40'

PARAMETER rho '360'
PARAMETER nu '0.0'
PARAMETER E '4.3E8'

PARAMETER neles  '10'
PARAMETER nnodos '9'

COORDINATES POINT SYSTEM=0
ENTRIES name x y z
1  0.00 0.00 $R
2  0.00 0.00 0.00
3  0.00 1.00 0.00

LINE ARC NAME=1 MODE=3 P1=1 P3=3 CENTER=2 RADIUS=$R ANGLE=$a
SURFACE EXTRUDED NAME=1 LINE=1 DX=$L DY=0 DZ=0
SUBDIVIDE LINE NAME=1 MODE=DIVISIONS NDIV=$neles RATIO=1.0
SUBDIVIDE LINE NAME=4 MODE=DIVISIONS NDIV=$neles RATIO=1.0
SUBDIVIDE LINE NAME=2 MODE=DIVISIONS NDIV=$neles RATIO=1.0
SUBDIVIDE LINE NAME=3 MODE=DIVISIONS NDIV=$neles RATIO=1.0

MATERIAL ELASTIC NAME=1,
                 E=$E,
                 NU=$nu,
                 DENSITY=1

EGROUP SHELL NAME=1,
             MATERIAL=1,
             RINT=3,
             SINT=3,
             TINT=3,
             THICKNESS=$t

GSURFACE NAME=1 NODES=$nnodos GROUP=1

FIXITY NAME=longSym
Y-TRANSLATION
X-ROTATION

FIXITY NAME=transSym
X-TRANSLATION
Y-ROTATION

FIXITY NAME=apoyo
Y-TRANSLATION
Z-TRANSLATION

FIXBOUNDARY LINES
1 apoyo
2 longSym
4 transSym

LOAD MASS-PROPORTIONAL NAME=1,
                       MAGNITUDE=$rho,
                       AX=0 AY=0 AZ=-1,
                       INTERPRETATION=BODY-FORCE

APPLY-LOAD
1 MASS-PROPORTIONAL 1 MODEL

MASTER SHELNDOF=5
